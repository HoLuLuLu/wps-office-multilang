<?xml version='1.0' encoding='utf-8'?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="application/wps-office.ppt">
		<comment>Microsoft Powerpoint</comment>
		<alias type="application/vnd.ms-powerpoint"/>
		<alias type="application/powerpoint"/>
		<alias type="application/mspowerpoint"/>
		<alias type="application/x-mspowerpoint"/>
		<alias type="application/vnd.ms-powerpoint.presentation.macroenabled.12"/>
		<generic-icon name="wps-office-ppt"/>
		<sub-class-of type="application/vnd.ms-powerpoint"/>
		<glob pattern="*.ppt" weight="60"/>
		<glob pattern="*.pptm" weight="60"/>
		<glob pattern="*.pps" weight="60"/>
	</mime-type>
	<mime-type type="application/wps-office.pot">
		<comment>Microsoft Powerpoint Template</comment>
		<alias type="application/vnd.ms-powerpoint"/>
		<alias type="application/powerpoint"/>
		<alias type="application/mspowerpoint"/>
		<alias type="application/x-mspowerpoint"/>
		<alias type="application/vnd.ms-powerpoint.template.macroenabled.12"/>
		<generic-icon name="wps-office-pot"/>
		<glob pattern="*.pot" weight="60"/>
		<glob pattern="*.potm" weight="60"/>
	</mime-type>
	<mime-type type="application/wps-office.potx">
		<comment>Microsoft Powerpoint Template</comment>
		<alias type="application/vnd.openxmlformats-officedocument.presentationml.template"/>
		<generic-icon name="wps-office-pot"/>
		<glob pattern="*.potx" weight="60"/>
	</mime-type>
	<mime-type type="application/wps-office.pptx">
		<comment>Microsoft Powerpoint</comment>
		<alias type="application/vnd.openxmlformats-officedocument.presentationml.presentation"/>
		<alias type="application/vnd.openxmlformats-officedocument.presentationml.slideshow"/>
		<generic-icon name="wps-office-ppt"/>
		<glob pattern="*.pptx" weight="60"/>
		<glob pattern="*.ppsx" weight="60"/>
	</mime-type>
	<mime-type type="application/wps-office.dps">
		<comment>WPS Presentation File</comment>
		<comment xml:lang="zh_CN">WPS演示 演示文稿</comment>
		<generic-icon name="wps-office-dps"/>
		<glob pattern="*.dps" weight="60"/>
	</mime-type>
	<mime-type type="application/wps-office.dpt">
		<comment>WPS Presentation Template</comment>
		<comment xml:lang="zh_CN">WPS演示 模板</comment>
		<generic-icon name="wps-office-dpt"/>
		<glob pattern="*.dpt" weight="60"/>
	</mime-type>
</mime-info>
